     formas_pagamentos.`tipo_credito` AS formas_pagamentos_tipo_credito
FROM
     `empresas` empresas INNER JOIN `facturas` facturas ON empresas.`id` = facturas.`empresa_id`
     INNER JOIN `factura_items` factura_items ON facturas.`id` = factura_items.`factura_id`
     INNER JOIN `clientes` clientes ON facturas.`cliente_id` = clientes.`id`
     LEFT JOIN `formas_pagamentos` formas_pagamentos ON facturas.`formas_pagamento_id` = formas_pagamentos.`id`
     AND empresas.`id` = formas_pagamentos.`empresa_id`
     AND empresas.`id` = clientes.`empresa_id`
WHERE empresas.id = $P{empresa_id}  AND DATE(facturas.created_at) = $P{data_inicio} AND (facturas.tipo_documento = 1
