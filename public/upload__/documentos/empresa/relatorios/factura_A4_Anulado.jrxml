     INNER JOIN `empresas` empresas ON produtos.`empresa_id` = empresas.`id`
     INNER JOIN `tipotaxa` tipotaxa ON produtos.`tipo_taxa_id` = tipotaxa.`id`
     AND empresas.`id` = facturas.`empresa_id`
     INNER JOIN `users` users ON empresas.`id` = users.`empresa_id`
     INNER JOIN `regimes` regimes ON empresas.`regime_id` = regimes.`id`
     INNER JOIN `regimes` regimes_A ON empresas.`regime_id` = regimes_A.`id`
     AND users.`id` = facturas.`user_id`
     INNER JOIN `clientes` clientes ON facturas.`cliente_id` = clientes.`id`
     INNER JOIN `tipo_documentos` tipo_documentos ON facturas.`tipo_documento_id` = tipo_documentos.`id`
     INNER JOIN `moedas` moedas ON facturas.`moeda_id` = moedas.`id`
     LEFT JOIN `formas_pagamentos` formas_pagamentos ON facturas.`forma_pagamento_id` = formas_pagamentos.`id`
