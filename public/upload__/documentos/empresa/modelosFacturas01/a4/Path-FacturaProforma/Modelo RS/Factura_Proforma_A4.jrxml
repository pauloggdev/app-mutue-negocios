	<queryString language="SQL">
		<![CDATA[SELECT
     UPPER(utilizadores.`Nome`) AS utilizadores_Nome,
     motivo.`descricacao` AS motivoIsencao,
     factura.`Codigo` AS factura_Codigo,
     factura.`DataFactura` AS factura_DataFactura,
     factura.`TotalPreco` AS factura_TotalPreco,
     factura.`ValorEntregue` AS factura_ValorEntregue,
     factura.`Desconto` AS factura_Desconto,
     factura.`ValorAPagar` AS factura_ValorAPagar,
     factura.`Troco` AS factura_Troco,
     factura.`NomeCliente` AS factura_NomeCliente,
     produtos.`Designacao` AS produtos_Designacao,
     produtos.`DescricaoProduto` AS produtos_DescricaoProdutos,
     produtos.`Preco` AS produtos_Preco,
     factura_items.`Quantidade` AS factura_items_Quantidade,
     factura_items.`Total` AS factura_items_Total,
     factura_items.`dia` AS factura_items_dia,
     factura_items.`TotalIPC` AS factura_items_valorIVA,
     factura_items.`descontoProduto` AS factura_itens_descontoProduto,
     armazens.`Designacao` AS armazens_Designacao,
     configuracoes.`nomeempresa` AS configuracoes_nomeempresa,
     configuracoes.`numcontribuinte` AS configuracoes_numcontribuinte,
     configuracoes.`endereco` AS configuracoes_endereco,
     configuracoes.`telefones` AS configuracoes_telefones,
     configuracoes.`telemoveis` AS configuracoes_telemoveis,
     configuracoes.`email` AS configuracoes_email,
          configuracoes.`cidade` AS configuracoes_cidade,
     configuracoes.`site` AS configuracoes_site,
     produtos.`CodigoCategoria` AS produtos_CodigoCategoria,
     categorias.`Designacao` AS categorias_Designacao,
     armazens.`Localizacao` AS armazens_Localizacao,
     factura.`morada` AS clientes_Morada,
     factura.`telefoneCliente` AS clientes_Telefone,
     factura.`emailCliente` AS clientes_Email,
     configuracoes.`nomebanco1` AS configuracoes_nomebanco1,
     configuracoes.`nomebanco2` AS configuracoes_nomebanco2,
     configuracoes.`nomebanco3` AS configuracoes_nomebanco3,
          configuracoes.`nomebanco4` AS configuracoes_nomebanco4,
     configuracoes.`nomebanco5` AS configuracoes_nomebanco5,
     configuracoes.`nomebanco6` AS configuracoes_nomebanco6,
          configuracoes.`nomebanco7` AS configuracoes_nomebanco7,
     configuracoes.`numeroconta1` AS configuracoes_numeroconta1,
     configuracoes.`numeroconta2` AS configuracoes_numeroconta2,
      configuracoes.`numeroconta3` AS configuracoes_numeroconta3,
           configuracoes.`numeroconta4` AS configuracoes_numeroconta4,
     configuracoes.`numeroconta5` AS configuracoes_numeroconta5,
      configuracoes.`numeroconta6` AS configuracoes_numeroconta6,
           configuracoes.`numeroconta7` AS configuracoes_numeroconta7,
     factura.`ValorAPagarExtenso` AS factura_ValorAPagarExtenso,
     factura_items.`OBS` AS factura_items_OBS,
     produtos.`DescricaoProduto` AS produtos_DescricaoProduto,
     factura.`nifCliente` AS clientes_NIF,
     factura.`Descricao` AS factura_Descricao,
     factura.`ValorDescricao` AS factura_ValorDescricao,
     formas_pagamento.`Designacao` AS formas_pagamento_Designacao,
     factura.`contaCorrente` AS clientes_ContaCorrente,
     clientes.`pessoaContacto` AS clientes_pessoaContacto,
     produtos.`CodigoProduto` AS produtos_CodigoProduto,
     factura_items.`preco` AS factura_items_preco,
     clientes.`Referencia` AS ref,
     clientes.`Ncontracto` AS contrato,
     factura.`retencaoV` AS factura_retencaoV,
     factura.`totalIVA` AS factura_totalIVA,
     factura.`ValorEntregueMltCX` AS factura_ValorEntregueMltCX,
     factura.`NextFactura` AS factura_NextFactura,
     tipotaxa.`taxa` AS taxaIVA,
     motivo.`descricacao` AS motivoIsencao,
     moedas.`cambio` AS cambio,
     moedas.`Designacao` AS moeda,
     unidade_medida.`designacao` AS unidadeMedida,
     factura.`hashValor` AS hashValor,
     factura.`faturaReference` AS faturaReference,
     factura.`dataVencimento` AS factura_dataVencimento,
     factura.`nEncomenda` AS factura_numeroEcomenda,
     factura.`obs` AS factura_observacao,
     serie.`Designacao` AS serie_Designacao,
     factura.`origemViagem` AS factura_origemViagem,
     factura.`destinoViagem` AS factura_destinoViagem,
     factura.`numContentor` AS factura_numContentor,
     factura.`motorista` AS factura_motorista,
     factura.`viatura` AS factura_viatura,
     configuracoes.`codigoTipoRegime` AS configuracoes_codigoTipoRegime,
     tipos_regimes.`Designacao` AS tipos_regimes_Designacao,
     factura_items.`Descricao` AS factura_items_Descricao,
     parametros.`Valor` AS parametros_Valor,
     factura.`cabecalho` AS factura_cabecalho,
     factura.`solicitacao` AS factura_solicitacao,
     factura_items.`retencao` AS factura_items_retencao
FROM
     `factura` factura INNER JOIN `factura_items` factura_items ON factura.`Codigo` = factura_items.`CodigoFactura`
     INNER JOIN `utilizadores` utilizadores ON factura.`CodigoUtilizador` = utilizadores.`Codigo`
     INNER JOIN `armazens` armazens ON factura.`CodigoArmazem` = armazens.`Codigo`
     INNER JOIN `clientes` clientes ON factura.`CodigoClientes` = clientes.`Codigo`
     INNER JOIN `formas_pagamento` formas_pagamento ON factura.`Forma` = formas_pagamento.`Codigo`
     INNER JOIN `moedas` moedas ON factura.`codigoMoeda` = moedas.`codigo`
     LEFT OUTER JOIN `serie` serie ON factura.`CodigoSerie` = serie.`Codigo`
     INNER JOIN `produtos` produtos ON factura_items.`CodigoProduto` = produtos.`Codigo`
     INNER JOIN `unidade_medida` unidade_medida ON produtos.`CodigoUnidadeMedida` = unidade_medida.`codigo`
     INNER JOIN `categorias` categorias ON produtos.`CodigoCategoria` = categorias.`Codigo`
     INNER JOIN `tipotaxa` tipotaxa ON produtos.`CodigoTipoTaxa` = tipotaxa.`Codigo`
     LEFT OUTER JOIN `motivo` motivo ON tipotaxa.`CodigoMotivo` = motivo.`Codigo`,
     `tipos_regimes` tipos_regimes INNER JOIN `configuracoes` configuracoes ON tipos_regimes.`Codigo` = configuracoes.`codigoTipoRegime`,
     `parametros` parametros

WHERE
     factura.`Codigo` = $P{CODIGO_FACTURA} and parametros.`codigo` = 14]]>
	</queryString>
